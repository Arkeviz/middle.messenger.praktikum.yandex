{
  "name": "middle.messenger.praktikum.yandex",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "lint:all": "run-p type-check lint lint:css",
    "lint": "eslint --fix",
    "lint:css": "npx stylelint \"**/*.{css,scss}\" --fix",
    "type-check": "tsc --noEmit",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "tsc && vite build && vite preview --port 3000"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.20.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "sass": "^1.84.0",
    "stylelint": "^16.15.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.0",
    "uuid": "^11.1.0",
    "vite": "^6.1.0"
  }
}
